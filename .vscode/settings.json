{
  "files.autoSave": "onWindowChange",
  "editor.renderWhitespace": "boundary",
  "editor.foldingStrategy": "indentation",
  "editor.gotoLocation.multiple": "gotoAndPeek",
  "editor.mouseWheelZoom": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontLigatures": true,
  "editor.suggest.localityBonus": true,
  "workbench.editor.highlightModifiedTabs": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "git.autofetch": true,
  "git.showPushSuccessNotification": true,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.enableScriptExplorer": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "'Operator Mono Lig', 'Sofia Pro Soft','Google Sans',Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "monospace",
  "vsicons.dontShowNewVersionMessage": true,
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.forceDownload": true,
  "sync.forceUpload": true,
  "sync.quietSync": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "git.confirmSync": false,
  "editor.tabSize": 2,
  "prettier.trailingComma": "all",
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": ["javascript", "javascriptreact", "javascriptvue"],
  "prettier.disableLanguages": [],
  "prettier.endOfLine": "lf",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
  "markdown.preview.breaks": true,
  "editor.wordWrapColumn": 100,
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "explorer.confirmDelete": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "html.format.indentHandlebars": true,
  "window.zoomLevel": 1,
  "python.jediEnabled": false,
  "terminal.integrated.shell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "workbench.colorTheme": "Hybrid Operator"
}
